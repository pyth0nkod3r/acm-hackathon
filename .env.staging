# Staging Environment Configuration
NODE_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://staging-api.afcfta-hackathon.com
VITE_API_USERNAME=staging_user
VITE_API_PASSWORD=staging_password

# Staging Configuration
VITE_USE_MOCK_API=false
VITE_BASE_URL=/

# Feature Flags
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_FORM_VALIDATION=true
VITE_ENABLE_DEBUG_MODE=true

# Performance
VITE_ENABLE_SOURCEMAPS=true
VITE_ENABLE_BUNDLE_ANALYZER=false

# Staging-specific settings
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=false